package in.co.sunrays.controller;

import java.text.SimpleDateFormat;

import jdk.nashorn.internal.runtime.regexp.joni.SearchAlgorithm;
import sun.text.resources.FormatData;

public class DataUtility1 {

	 
	 public static final String APP_DATE_FORMAT1 = "dd/mm/yyyy";
	 
	    
	    
	    public static final String SERCH_DATE_FORMAT1 = "yyyy-MM-dd";
	    

	    
        public static final String APP_TIME_FORMAT1= "dd/MM/yyyy HH:mm:ss";


	
	   private static final SimpleDateFormat Formatter = new SimpleDateFormat(APP_DATE_FORMAT1);
	   
	   private static final SimpleDateFormat Formatter2 = new SimpleDateFormat(SERCH_DATE_FORMAT1);
	   
	   private static final SimpleDateFormat timeFormatter = new SimpleDateFormat(APP_TIME_FORMAT1);

	   
	   public static String getString(String val) {
		   if(DataValidator1.isNotNull(val)) {
			   return val.trim();
		   } else {
			   return val;
		   }
	   }

       
	   public static String getStringData(Object Val) {
		   if(Val != null) {
			   return Val.toString();
		   } else {
			   return "";
		   }
	   }
		   
	   }



























}	   
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

